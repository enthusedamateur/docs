#https://tttapa.github.io/Pages/Raspberry-Pi/Installation+Setup/WiFi-Setup.html

sudo nano /etc/netplan/10-my-config.yaml

  network:
  version: 2
  ethernets:
    eth0:
      dhcp4: true
      optional: true
  wifis:
    wlan0:
      dhcp4: true
      optional: true
      access-points:
        "Your-WiFi-Network-Name":
          password: "******"
          
sudo netplan generate
